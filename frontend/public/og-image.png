<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 630">
  <defs>
    <style>
      .cyber-dark { fill: #0B0F1A; }
      .cyber-blue { fill: #3B82F6; }
      .cyber-text { font-family: 'Orbitron', monospace; font-size: 72px; fill: #60A5FA; font-weight: bold; }
    </style>
  </defs>
  <!-- Background -->
  <rect width="1200" height="630" class="cyber-dark"/>
  
  <!-- Grid pattern -->
  <defs>
    <pattern id="grid" width="60" height="60" patternUnits="userSpaceOnUse">
      <path d="M 60 0 L 0 0 0 60" fill="none" stroke="#3B82F6" stroke-width="0.5" opacity="0.1"/>
    </pattern>
  </defs>
  <rect width="1200" height="630" fill="url(#grid)"/>
  
  <!-- Gradient overlay -->
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3B82F6;stop-opacity:0.1" />
      <stop offset="100%" style="stop-color:#0B0F1A;stop-opacity:0.9" />
    </linearGradient>
  </defs>
  <rect width="1200" height="630" fill="url(#grad)"/>
  
  <!-- Text -->
  <text x="600" y="315" class="cyber-text" text-anchor="middle" dominant-baseline="middle">
    CM.DEV
  </text>
  
  <!-- Corner accents -->
  <g stroke="#3B82F6" stroke-width="3" opacity="0.6">
    <line x1="50" y1="50" x2="150" y2="50"/>
    <line x1="50" y1="50" x2="50" y2="150"/>
    
    <line x1="1150" y1="50" x2="1050" y2="50"/>
    <line x1="1150" y1="50" x2="1150" y2="150"/>
    
    <line x1="50" y1="580" x2="150" y2="580"/>
    <line x1="50" y1="580" x2="50" y2="480"/>
    
    <line x1="1150" y1="580" x2="1050" y2="580"/>
    <line x1="1150" y1="580" x2="1150" y2="480"/>
  </g>
</svg>
